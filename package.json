    "prettier": "2.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rollup": "^1.27.4",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^26.4.4",
    "typescript": "^3.7.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn build && yarn prettier --write '*/**/*.{js,ts,tsx}'",
